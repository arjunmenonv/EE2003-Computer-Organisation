    %jmp T_3.11;
T_3.10 ;
    %mov 8, 2, 8;
T_3.11 ;
; Save base=8 wid=8 in lookaside.
    %ix/getv 3, v0x9404d0_0;
   %jmp/1 t_1, 4;
