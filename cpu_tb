    .scope S_0x90dad0;
T_3 ;
    %wait E_0x8fbe10;
    %ix/load 1, 3, 0;
    %mov 4, 0, 1;
    %jmp/1 T_3.0, 4;
    %load/x1p 8, v0x940880_0, 1;
    %jmp T_3.1;
T_3.0 ;
    %mov 8, 2, 1;
T_3.1 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_3.2, 8;
    %ix/load 1, 24, 0;
    %mov 4, 0, 1;
    %jmp/1 T_3.4, 4;
    %load/x1p 8, v0x9407e0_0, 8;
    %jmp T_3.5;
T_3.4 ;
    %mov 8, 2, 8;
T_3.5 ;
; Save base=8 wid=8 in lookaside.
    %ix/getv 3, v0x9404d0_0;
   %jmp/1 t_0, 4;
   %ix/load 1, 0, 0;
   %set/av v0x940b70, 8, 8;
t_0 ;
T_3.2 ;
    %ix/load 1, 2, 0;
    %mov 4, 0, 1;
