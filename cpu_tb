v0x13c2b70 .array "mem3", 4095 0, 7 0;
E_0x137de10 .event posedge, v0x13c2570_0;
L_0x13c42d0 .part v0x13c3220_0, 2, 30;
L_0x13c4370 .array/port v0x13c2b70, L_0x13c42d0;
L_0x13c4460 .array/port v0x13c2af0, L_0x13c42d0;
L_0x13c4500 .array/port v0x13c2a00, L_0x13c42d0;
L_0x13c4630 .array/port v0x13c2980, L_0x13c42d0;
L_0x13c46d0 .concat [ 8 8 8 8], L_0x13c4630, L_0x13c4500, L_0x13c4460, L_0x13c4370;
    .scope S_0x13c3060;
T_0 ;
    %wait E_0x137de10;
    %load/v 8, v0x13c3640_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 32, 0;
    %assign/v0 v0x13c3510_0, 0, 0;
    %ix/load 0, 32, 0;
    %assign/v0 v0x13c3220_0, 0, 0;
    %ix/load 0, 32, 0;
    %assign/v0 v0x13c3380_0, 0, 0;
    %ix/load 0, 4, 0;
    %assign/v0 v0x13c3460_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x13c3510_0, 32;
    %mov 40, 0, 1;
    %addi 8, 1, 33;
    %ix/load 0, 32, 0;
    %assign/v0 v0x13c3510_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x13c2c70;
T_1 ;
    %vpi_call 4 9 "$readmemh", "./test/t1/idata.mem", v0x13c2fe0;
    %end;
    .thread T_1;
    .scope S_0x138fad0;
T_2 ;
    %vpi_call 5 21 "$readmemh", "./test/t1/data0.mem", v0x13c2980;
    %vpi_call 5 22 "$readmemh", "./test/t1/data1.mem", v0x13c2a00;
    %vpi_call 5 23 "$readmemh", "./test/t1/data2.mem", v0x13c2af0;
    %vpi_call 5 24 "$readmemh", "./test/t1/data3.mem", v0x13c2b70;
    %end;
    .thread T_2;
    .scope S_0x138fad0;
T_3 ;
    %wait E_0x137de10;
    %ix/load 1, 3, 0;
    %mov 4, 0, 1;
    %jmp/1 T_3.0, 4;
    %load/x1p 8, v0x13c2880_0, 1;
    %jmp T_3.1;
T_3.0 ;
    %mov 8, 2, 1;
T_3.1 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_3.2, 8;
    %ix/load 1, 24, 0;
    %mov 4, 0, 1;
    %jmp/1 T_3.4, 4;
    %load/x1p 8, v0x13c27e0_0, 8;
    %jmp T_3.5;
T_3.4 ;
    %mov 8, 2, 8;
T_3.5 ;
; Save base=8 wid=8 in lookaside.
    %ix/getv 3, v0x13c24d0_0;
   %jmp/1 t_0, 4;
   %ix/load 1, 0, 0;
   %set/av v0x13c2b70, 8, 8;
t_0 ;
T_3.2 ;
    %ix/load 1, 2, 0;
    %mov 4, 0, 1;
    %jmp/1 T_3.6, 4;
    %load/x1p 8, v0x13c2880_0, 1;
    %jmp T_3.7;
T_3.6 ;
    %mov 8, 2, 1;
T_3.7 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_3.8, 8;
    %ix/load 1, 16, 0;
    %mov 4, 0, 1;
    %jmp/1 T_3.10, 4;
    %load/x1p 8, v0x13c27e0_0, 8;
    %jmp T_3.11;
T_3.10 ;
    %mov 8, 2, 8;
T_3.11 ;
; Save base=8 wid=8 in lookaside.
    %ix/getv 3, v0x13c24d0_0;
   %jmp/1 t_1, 4;
   %ix/load 1, 0, 0;
   %set/av v0x13c2af0, 8, 8;
t_1 ;
T_3.8 ;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_3.12, 4;
    %load/x1p 8, v0x13c2880_0, 1;
    %jmp T_3.13;
T_3.12 ;
    %mov 8, 2, 1;
T_3.13 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_3.14, 8;
    %ix/load 1, 8, 0;
    %mov 4, 0, 1;
    %jmp/1 T_3.16, 4;
    %load/x1p 8, v0x13c27e0_0, 8;
    %jmp T_3.17;
T_3.16 ;
    %mov 8, 2, 8;
T_3.17 ;
; Save base=8 wid=8 in lookaside.
    %ix/getv 3, v0x13c24d0_0;
   %jmp/1 t_2, 4;
   %ix/load 1, 0, 0;
   %set/av v0x13c2a00, 8, 8;
t_2 ;
T_3.14 ;
    %load/v 8, v0x13c2880_0, 1; Only need 1 of 4 bits
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_3.18, 8;
    %load/v 8, v0x13c27e0_0, 8; Only need 8 of 32 bits
; Save base=8 wid=8 in lookaside.
    %ix/getv 3, v0x13c24d0_0;
   %jmp/1 t_3, 4;
   %ix/load 1, 0, 0;
   %set/av v0x13c2980, 8, 8;
t_3 ;
T_3.18 ;
    %jmp T_3;
