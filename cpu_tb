#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x14bf580 .scope module, "cpu_tb" "cpu_tb" 2 12;
 .timescale -9 -9;
v0x15046d0_0 .var "clk", 0 0;
v0x15047a0_0 .net "daddr", 31 0, v0x1504230_0; 1 drivers
v0x1504870_0 .net "drdata", 31 0, L_0x15056e0; 1 drivers
v0x1504940_0 .var "dtmp", 31 0;
v0x15049c0_0 .net "dwdata", 31 0, v0x1504390_0; 1 drivers
v0x1504a90_0 .net "dwe", 3 0, v0x1504470_0; 1 drivers
v0x1504b60_0 .var/i "exp_file", 31 0;
v0x1504be0_0 .var "exp_reg", 31 0;
v0x1504cb0_0 .var/i "fail", 31 0;
v0x1504d30_0 .var/i "i", 31 0;
v0x1504db0_0 .net "iaddr", 31 0, v0x1504520_0; 1 drivers
v0x1504e30_0 .net "idata", 31 0, L_0x1503910; 1 drivers
v0x1504f00_0 .var/i "log_file", 31 0;
v0x1504f80_0 .var "reset", 0 0;
v0x1505080_0 .var/i "s", 31 0;
S_0x1504070 .scope module, "u1" "cpu" 2 21, 3 1, S_0x14bf580;
 .timescale 0 0;
v0x1504160_0 .net "clk", 0 0, v0x15046d0_0; 1 drivers
v0x1504230_0 .var "daddr", 31 0;
v0x15042e0_0 .alias "drdata", 31 0, v0x1504870_0;
v0x1504390_0 .var "dwdata", 31 0;
v0x1504470_0 .var "dwe", 3 0;
v0x1504520_0 .var "iaddr", 31 0;
v0x15045a0_0 .alias "idata", 31 0, v0x1504e30_0;
v0x1504650_0 .net "reset", 0 0, v0x1504f80_0; 1 drivers
S_0x1503c80 .scope module, "u2" "imem" 2 32, 4 3, S_0x14bf580;
 .timescale -9 -12;
L_0x1503910 .functor BUFZ 32, L_0x1505100, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>;
v0x1503d70_0 .net *"_s0", 31 0, L_0x1505100; 1 drivers
v0x1503e30_0 .net *"_s3", 29 0, L_0x15051a0; 1 drivers
v0x1503ed0_0 .alias "iaddr", 31 0, v0x1504db0_0;
v0x1503f70_0 .alias "idata", 31 0, v0x1504e30_0;
v0x1503ff0 .array "mem", 4095 0, 31 0;
L_0x1505100 .array/port v0x1503ff0, L_0x15051a0;
L_0x15051a0 .part v0x1504520_0, 2, 30;
S_0x14d0ad0 .scope module, "u3" "dmem" 2 37, 5 4, S_0x14bf580;
 .timescale -9 -12;
v0x14e0140_0 .net *"_s2", 7 0, L_0x1505380; 1 drivers
v0x15032f0_0 .net *"_s4", 7 0, L_0x1505470; 1 drivers
v0x1503390_0 .net *"_s6", 7 0, L_0x1505510; 1 drivers
v0x1503430_0 .net *"_s8", 7 0, L_0x1505640; 1 drivers
v0x15034e0_0 .net "a", 29 0, L_0x15052e0; 1 drivers
v0x1503580_0 .alias "clk", 0 0, v0x1504160_0;
v0x1503660_0 .alias "daddr", 31 0, v0x15047a0_0;
v0x1503700_0 .alias "drdata", 31 0, v0x1504870_0;
v0x15037f0_0 .alias "dwdata", 31 0, v0x15049c0_0;
v0x1503890_0 .alias "dwe", 3 0, v0x1504a90_0;
v0x1503990 .array "mem0", 4095 0, 7 0;
v0x1503a10 .array "mem1", 4095 0, 7 0;
v0x1503b00 .array "mem2", 4095 0, 7 0;
v0x1503b80 .array "mem3", 4095 0, 7 0;
E_0x14bee10 .event posedge, v0x1503580_0;
L_0x15052e0 .part v0x1504230_0, 2, 30;
L_0x1505380 .array/port v0x1503b80, L_0x15052e0;
L_0x1505470 .array/port v0x1503b00, L_0x15052e0;
L_0x1505510 .array/port v0x1503a10, L_0x15052e0;
L_0x1505640 .array/port v0x1503990, L_0x15052e0;
L_0x15056e0 .concat [ 8 8 8 8], L_0x1505640, L_0x1505510, L_0x1505470, L_0x1505380;
    .scope S_0x1504070;
T_0 ;
    %wait E_0x14bee10;
    %load/v 8, v0x1504650_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 32, 0;
    %assign/v0 v0x1504520_0, 0, 0;
    %ix/load 0, 32, 0;
    %assign/v0 v0x1504230_0, 0, 0;
    %ix/load 0, 32, 0;
    %assign/v0 v0x1504390_0, 0, 0;
    %ix/load 0, 4, 0;
    %assign/v0 v0x1504470_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x1504520_0, 32;
    %mov 40, 0, 1;
    %addi 8, 1, 33;
    %ix/load 0, 32, 0;
    %assign/v0 v0x1504520_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1503c80;
T_1 ;
    %vpi_call 4 9 "$readmemh", "./test/t1/idata.mem", v0x1503ff0;
    %end;
    .thread T_1;
    .scope S_0x14d0ad0;
T_2 ;
    %vpi_call 5 21 "$readmemh", "./test/t1/data0.mem", v0x1503990;
    %vpi_call 5 22 "$readmemh", "./test/t1/data1.mem", v0x1503a10;
    %vpi_call 5 23 "$readmemh", "./test/t1/data2.mem", v0x1503b00;
    %vpi_call 5 24 "$readmemh", "./test/t1/data3.mem", v0x1503b80;
    %end;
    .thread T_2;
    .scope S_0x14d0ad0;
T_3 ;
    %wait E_0x14bee10;
    %ix/load 1, 3, 0;
    %mov 4, 0, 1;
    %jmp/1 T_3.0, 4;
    %load/x1p 8, v0x1503890_0, 1;
    %jmp T_3.1;
T_3.0 ;
    %mov 8, 2, 1;
T_3.1 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_3.2, 8;
    %ix/load 1, 24, 0;
    %mov 4, 0, 1;
    %jmp/1 T_3.4, 4;
    %load/x1p 8, v0x15037f0_0, 8;
    %jmp T_3.5;
T_3.4 ;
    %mov 8, 2, 8;
T_3.5 ;
; Save base=8 wid=8 in lookaside.
    %ix/getv 3, v0x15034e0_0;
   %jmp/1 t_0, 4;
   %ix/load 1, 0, 0;
   %set/av v0x1503b80, 8, 8;
t_0 ;
T_3.2 ;
    %ix/load 1, 2, 0;
    %mov 4, 0, 1;
    %jmp/1 T_3.6, 4;
    %load/x1p 8, v0x1503890_0, 1;
    %jmp T_3.7;
T_3.6 ;
    %mov 8, 2, 1;
T_3.7 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_3.8, 8;
    %ix/load 1, 16, 0;
    %mov 4, 0, 1;
    %jmp/1 T_3.10, 4;
    %load/x1p 8, v0x15037f0_0, 8;
    %jmp T_3.11;
T_3.10 ;
    %mov 8, 2, 8;
T_3.11 ;
; Save base=8 wid=8 in lookaside.
    %ix/getv 3, v0x15034e0_0;
   %jmp/1 t_1, 4;
   %ix/load 1, 0, 0;
   %set/av v0x1503b00, 8, 8;
t_1 ;
T_3.8 ;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_3.12, 4;
    %load/x1p 8, v0x1503890_0, 1;
    %jmp T_3.13;
T_3.12 ;
    %mov 8, 2, 1;
T_3.13 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_3.14, 8;
    %ix/load 1, 8, 0;
    %mov 4, 0, 1;
    %jmp/1 T_3.16, 4;
    %load/x1p 8, v0x15037f0_0, 8;
    %jmp T_3.17;
T_3.16 ;
    %mov 8, 2, 8;
T_3.17 ;
; Save base=8 wid=8 in lookaside.
    %ix/getv 3, v0x15034e0_0;
   %jmp/1 t_2, 4;
   %ix/load 1, 0, 0;
   %set/av v0x1503a10, 8, 8;
t_2 ;
T_3.14 ;
    %load/v 8, v0x1503890_0, 1; Only need 1 of 4 bits
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_3.18, 8;
    %load/v 8, v0x15037f0_0, 8; Only need 8 of 32 bits
; Save base=8 wid=8 in lookaside.
    %ix/getv 3, v0x15034e0_0;
   %jmp/1 t_3, 4;
   %ix/load 1, 0, 0;
   %set/av v0x1503990, 8, 8;
t_3 ;
T_3.18 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x14bf580;
T_4 ;
    %delay 5000, 0;
    %load/v 8, v0x15046d0_0, 1;
    %inv 8, 1;
    %set/v v0x15046d0_0, 8, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x14bf580;
T_5 ;
    %vpi_call 2 53 "$display", "RUNNING TEST FROM ", "./test/t1";
    %set/v v0x15046d0_0, 1, 1;
    %set/v v0x1504f80_0, 1, 1;
    %delay 100000, 0;
    %set/v v0x1504f80_0, 0, 1;
    %vpi_func 2 58 "$fopen", 8, 32, "cpu_tb.log", "a";
    %set/v v0x1504f00_0, 8, 32;
    %vpi_func 2 59 "$fopen", 8, 32, "./test/t1/expout.txt", "r";
    %set/v v0x1504b60_0, 8, 32;
    %wait E_0x14bee10;
    %set/v v0x1504d30_0, 0, 32;
T_5.0 ;
    %load/v 8, v0x1504d30_0, 32;
   %cmpi/s 8, 1000, 32;
    %jmp/0xz T_5.1, 5;
    %wait E_0x14bee10;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x1504d30_0, 32;
    %set/v v0x1504d30_0, 8, 32;
    %jmp T_5.0;
T_5.1 ;
    %set/v v0x1504cb0_0, 0, 32;
    %set/v v0x1504d30_0, 0, 32;
T_5.2 ;
    %load/v 8, v0x1504d30_0, 32;
   %cmpi/s 8, 32, 32;
    %jmp/0xz T_5.3, 5;
    %vpi_func 2 68 "$fscanf", 8, 32, v0x1504b60_0, "%d\012", v0x1504be0_0;
    %set/v v0x1505080_0, 8, 32;
    %ix/getv/s 3, v0x1504d30_0;
    %load/av 8, v0x1503990, 8;
    %ix/getv/s 3, v0x1504d30_0;
    %load/av 16, v0x1503a10, 8;
    %ix/getv/s 3, v0x1504d30_0;
    %load/av 24, v0x1503b00, 8;
    %ix/getv/s 3, v0x1504d30_0;
    %load/av 32, v0x1503b80, 8;
    %set/v v0x1504940_0, 8, 32;
    %load/v 8, v0x1504be0_0, 32;
    %load/v 40, v0x1504940_0, 32;
    %cmp/u 8, 40, 32;
    %inv 4, 1;
    %jmp/0xz  T_5.4, 4;
    %load/v 8, v0x1504be0_0, 32;
    %vpi_call 2 71 "$display", "FAIL: Expected Reg[%d] = %x vs. Got Reg[%d] = %x", v0x1504d30_0, T<8,32,s>, v0x1504d30_0, v0x1504940_0;
    %load/v 8, v0x1504cb0_0, 32;
    %mov 40, 39, 1;
    %addi 8, 1, 33;
    %set/v v0x1504cb0_0, 8, 32;
T_5.4 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x1504d30_0, 32;
    %set/v v0x1504d30_0, 8, 32;
    %jmp T_5.2;
T_5.3 ;
    %load/v 8, v0x1504cb0_0, 32;
    %cmpi/u 8, 0, 32;
    %inv 4, 1;
    %jmp/0xz  T_5.6, 4;
    %vpi_call 2 76 "$display", "FAILED. %d registers do not match.\012", v0x1504cb0_0;
    %vpi_call 2 77 "$fwrite", v0x1504f00_0, "FAIL\012";
    %jmp T_5.7;
T_5.6 ;
    %vpi_call 2 79 "$fwrite", v0x1504f00_0, "PASS\012";
T_5.7 ;
    %vpi_call 2 81 "$finish";
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
