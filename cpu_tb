    %set/v v0x941ef0_0, 8, 32;
    %vpi_func 2 59 "$fopen", 8, 32, "./test/t1/expout.txt", "r";
    %set/v v0x941b50_0, 8, 32;
    %wait E_0x8fbe10;
    %set/v v0x941d20_0, 0, 32;
T_5.0 ;
    %load/v 8, v0x941d20_0, 32;
   %cmpi/s 8, 1000, 32;
    %jmp/0xz T_5.1, 5;
    %wait E_0x8fbe10;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x941d20_0, 32;
    %set/v v0x941d20_0, 8, 32;
    %jmp T_5.0;
T_5.1 ;
    %set/v v0x941ca0_0, 0, 32;
    %movi 8, 10, 32;
    %set/v v0x941d20_0, 8, 32;
T_5.2 ;
    %load/v 8, v0x941d20_0, 32;
   %cmpi/s 8, 11, 32;
    %jmp/0xz T_5.3, 5;
    %vpi_func 2 69 "$fscanf", 8, 32, v0x941b50_0, "%d\012", v0x941bd0_0;
    %set/v v0x942070_0, 8, 32;
    %ix/getv/s 3, v0x941d20_0;
    %load/av 8, v0x940980, 8;
    %ix/getv/s 3, v0x941d20_0;
    %load/av 16, v0x940a00, 8;
    %ix/getv/s 3, v0x941d20_0;
    %load/av 24, v0x940af0, 8;
    %ix/getv/s 3, v0x941d20_0;
    %load/av 32, v0x940b70, 8;
    %set/v v0x941930_0, 8, 32;
    %load/v 8, v0x941bd0_0, 32;
    %load/v 40, v0x941930_0, 32;
    %cmp/u 8, 40, 32;
    %inv 4, 1;
    %jmp/0xz  T_5.4, 4;
    %load/v 8, v0x941bd0_0, 32;
    %vpi_call 2 72 "$display", "FAIL: Expected Reg[%d] = %x vs. Got Reg[%d] = %x", v0x941d20_0, T<8,32,s>, v0x941d20_0, v0x941930_0;
    %load/v 8, v0x941ca0_0, 32;
    %mov 40, 39, 1;
    %addi 8, 1, 33;
    %set/v v0x941ca0_0, 8, 32;
T_5.4 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x941d20_0, 32;
    %set/v v0x941d20_0, 8, 32;
    %jmp T_5.2;
T_5.3 ;
    %load/v 8, v0x941ca0_0, 32;
    %cmpi/u 8, 0, 32;
    %inv 4, 1;
    %jmp/0xz  T_5.6, 4;
    %vpi_call 2 78 "$display", "FAILED. %d registers do not match.\012", v0x941ca0_0;
    %vpi_call 2 79 "$fwrite", v0x941ef0_0, "FAIL\012";
    %jmp T_5.7;
T_5.6 ;
    %vpi_call 2 81 "$fwrite", v0x941ef0_0, "PASS\012";
T_5.7 ;
    %vpi_call 2 83 "$finish";
    %end;
    .thread T_5;
