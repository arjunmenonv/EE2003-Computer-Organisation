    .scope S_0x137e580;
T_4 ;
    %delay 5000, 0;
    %load/v 8, v0x13c36c0_0, 1;
    %inv 8, 1;
    %set/v v0x13c36c0_0, 8, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x137e580;
T_5 ;
    %vpi_call 2 53 "$display", "RUNNING TEST FROM ", "./test/t1";
    %set/v v0x13c36c0_0, 1, 1;
    %set/v v0x13c3f70_0, 1, 1;
    %delay 100000, 0;
    %set/v v0x13c3f70_0, 0, 1;
    %vpi_func 2 58 "$fopen", 8, 32, "cpu_tb.log", "a";
    %set/v v0x13c3ef0_0, 8, 32;
    %vpi_func 2 59 "$fopen", 8, 32, "./test/t1/expout.txt", "r";
    %set/v v0x13c3b50_0, 8, 32;
    %wait E_0x137de10;
    %set/v v0x13c3d20_0, 0, 32;
T_5.0 ;
    %load/v 8, v0x13c3d20_0, 32;
   %cmpi/s 8, 1000, 32;
    %jmp/0xz T_5.1, 5;
    %wait E_0x137de10;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x13c3d20_0, 32;
    %set/v v0x13c3d20_0, 8, 32;
    %jmp T_5.0;
T_5.1 ;
    %set/v v0x13c3ca0_0, 0, 32;
    %movi 8, 10, 32;
    %set/v v0x13c3d20_0, 8, 32;
T_5.2 ;
    %load/v 8, v0x13c3d20_0, 32;
   %cmpi/s 8, 11, 32;
    %jmp/0xz T_5.3, 5;
    %vpi_func 2 69 "$fscanf", 8, 32, v0x13c3b50_0, "%d\012", v0x13c3bd0_0;
    %set/v v0x13c4070_0, 8, 32;
    %ix/getv/s 3, v0x13c3d20_0;
    %load/av 8, v0x13c2980, 8;
    %ix/getv/s 3, v0x13c3d20_0;
    %load/av 16, v0x13c2a00, 8;
    %ix/getv/s 3, v0x13c3d20_0;
    %load/av 24, v0x13c2af0, 8;
    %ix/getv/s 3, v0x13c3d20_0;
    %load/av 32, v0x13c2b70, 8;
    %set/v v0x13c3930_0, 8, 32;
    %load/v 8, v0x13c3bd0_0, 32;
    %load/v 40, v0x13c3930_0, 32;
    %cmp/u 8, 40, 32;
    %inv 4, 1;
    %jmp/0xz  T_5.4, 4;
    %load/v 8, v0x13c3bd0_0, 32;
    %vpi_call 2 72 "$display", "FAIL: Expected Reg[%d] = %x vs. Got Reg[%d] = %x", v0x13c3d20_0, T<8,32,s>, v0x13c3d20_0, v0x13c3930_0;
    %load/v 8, v0x13c3ca0_0, 32;
    %mov 40, 39, 1;
    %addi 8, 1, 33;
    %set/v v0x13c3ca0_0, 8, 32;
T_5.4 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x13c3d20_0, 32;
    %set/v v0x13c3d20_0, 8, 32;
    %jmp T_5.2;
T_5.3 ;
    %load/v 8, v0x13c3ca0_0, 32;
    %cmpi/u 8, 0, 32;
    %inv 4, 1;
    %jmp/0xz  T_5.6, 4;
    %vpi_call 2 78 "$display", "FAILED. %d registers do not match.\012", v0x13c3ca0_0;
    %vpi_call 2 79 "$fwrite", v0x13c3ef0_0, "FAIL\012";
    %jmp T_5.7;
T_5.6 ;
    %vpi_call 2 81 "$fwrite", v0x13c3ef0_0, "PASS\012";
T_5.7 ;
    %vpi_call 2 83 "$finish";
    %end;
    .thread T_5;
