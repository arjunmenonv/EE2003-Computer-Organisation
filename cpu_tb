    %set/v v0x18d7080_0, 8, 32;
    %ix/getv/s 3, v0x18d6d30_0;
    %load/av 8, v0x18d5990, 8;
    %ix/getv/s 3, v0x18d6d30_0;
    %load/av 16, v0x18d5a10, 8;
    %ix/getv/s 3, v0x18d6d30_0;
    %load/av 24, v0x18d5b00, 8;
    %ix/getv/s 3, v0x18d6d30_0;
    %load/av 32, v0x18d5b80, 8;
    %set/v v0x18d6940_0, 8, 32;
    %load/v 8, v0x18d6be0_0, 32;
    %load/v 40, v0x18d6940_0, 32;
    %cmp/u 8, 40, 32;
    %inv 4, 1;
    %jmp/0xz  T_5.4, 4;
    %load/v 8, v0x18d6be0_0, 32;
    %vpi_call 2 71 "$display", "FAIL: Expected Reg[%d] = %x vs. Got Reg[%d] = %x", v0x18d6d30_0, T<8,32,s>, v0x18d6d30_0, v0x18d6940_0;
    %load/v 8, v0x18d6cb0_0, 32;
    %mov 40, 39, 1;
    %addi 8, 1, 33;
    %set/v v0x18d6cb0_0, 8, 32;
T_5.4 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x18d6d30_0, 32;
    %set/v v0x18d6d30_0, 8, 32;
    %jmp T_5.2;
T_5.3 ;
    %load/v 8, v0x18d6cb0_0, 32;
    %cmpi/u 8, 0, 32;
    %inv 4, 1;
    %jmp/0xz  T_5.6, 4;
    %vpi_call 2 77 "$display", "FAILED. %d registers do not match.\012", v0x18d6cb0_0;
